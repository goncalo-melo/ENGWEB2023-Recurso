extends layout

block content
  .w3-card-4
    header.w3-container.w3-indigo
      h1 Lista de Consultas
    .w3-container
      table.w3-table-all
        tr
          th Id
          th Data 
          th Nome
          th Idade
          th Sexo 
          th Número de Intervenções

        each c in clist
          tr
            td
              a(href="/" + c._id) #{c._id}
            td= c.data
            td= c.nome
            td= c.idade
            td= c.sexo
            td= c.nr_operacoes

    footer.w3-container.w3-indigo
      h5 Generated by EngWeb in #{d}